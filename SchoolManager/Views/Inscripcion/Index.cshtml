@model IEnumerable<SchoolSharedInfo.Models.FechaInscripcion>

@{
    ViewBag.Title = "Index";
}

<h2>Realizar Nueva Inscripción</h2>

<p>
    @Html.ActionLink("Asignar Cursos", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CursoID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AlumnoID)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Curso.NombreCurso)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Alumno.Nombre)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.FechaInscripcionID }) |
            @Html.ActionLink("Details", "Details", new { id=item.FechaInscripcionID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.FechaInscripcionID })
        </td>
    </tr>
}

</table>
